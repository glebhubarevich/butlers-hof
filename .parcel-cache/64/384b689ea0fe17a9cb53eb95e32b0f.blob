.floating-btn {
  position: fixed;
  z-index: 100;
  bottom: 20px;
  right: 20px;
  height: 78px;
  width: 78px;
  background: #ea923a;
  border-radius: 50%;
  transform: scale(100%);
  transition: all ease 0.3s;
  box-shadow: 0px 4px 4px rgba(0, 0, 0, 0.11);
  opacity: 80%;
}
.floating-btn img {
  position: absolute;
  top: 50%;
  left: 50%;
  height: 47px;
  width: 47%;
  transform: translate(-50%, -50%) scale(95%);
