.header {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  height: min(16vh, 9rem);
  width: 100vw;
  background: rgba(36, 35, 33, 0.7);
  backdrop-filter: blur(2px);
  position: fixed;
  top: 0;
  left: 0;
  z-index: 10;
  transform: translateY(-100%);
  transition: translateX 0.5s ease;
}
.header-active {
  transform: translateY(0);
}
.header__items {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  justify-content: space-evenly;
  height: 100%;
  width: 90%;
}
.header__items_group {
  position: relative;
  display: flex;
  justify-content: center;
  width: 30rem;
}
.header__items_group::after {
  content: "";
  position: absolute;
  width: 120%;
  height: 1px;
  background-color: #ea923a;
  top: 120%;
  left: 50%;
  transform: translateX(-50%);
}
.header__items_item a,
.header__items_item a:active {
  color: #faf5f1;
  margin: 0 3rem;
  text-transform: uppercase;
  font-size: 1.4vw;
  white-space: nowrap;
  transition: color 0.3s ease;
}
.header__items_item a:hover,
.header__items_item a:active:hover {
  color: #ea923a;
}
.header__logo {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 90%;
}
.header__logo a {
  display: block;
  height: 100%;
}
.header__logo img {
  height: 100%;
}
.h-section {
  width: 100%;
  height: clamp(44vw, 72rem, 80vh);
  display: grid;
  align-items: center;
  justify-items: center;
  grid-template-columns: 1fr 1.6fr;
  grid-template-areas: "image content";
  border: 2px #242321;
  border-bottom-style: solid;
}
.h-section:nth-child(odd) {
  color: #faf5f1;
  background: #242321;
  grid-template-columns: 1.6fr 1fr;
  grid-template-areas: "content image";
}
.h-section:nth-child(odd) .button-outline {
  color: #faf5f1;
  border: solid #faf5f1 2px;
}
.h-section-img {
  grid-area: image;
  width: 100%;
  height: 100%;
  background-size: cover !important;
  background-repeat: no-repeat !important;
}
.h-section-content {
  grid-area: content;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  height: 80%;
  width: 70%;
  font-size: 4.5vw;
}
.h-section-content h2 {
  font-size: min(1em, 7rem);
  line-height: 150%;
  text-transform: uppercase;
}
.h-section-content p {
  font-size: min(0.375em, 2.2rem);
  line-height: 150%;
}
.h-section-content a {
  font-size: min(0.375em, 2.2rem);
  line-height: 150%;
  color: inherit;
  text-decoration: underline;
}
.h-section-content h4 {
  font-size: min(0.5em, 3.5rem);
  line-height: 150%;
  margin-top: 2rem;
  color: inherit;
}
.button-outline {
  background: none;
  color: #242321;
  border: solid #242321 2px;
  font-size: min(0.375em, 2.2rem);
  padding: 0.8em 1.2em;
  margin-top: 80px;
  transition: all ease 0.3s;
  text-transform: uppercase;
  text-decoration: none !important;
}
.button-outline:hover {
  background: #ea923a;
  color: #faf5f1;
  transform: scale(105%);
}
.button-outline-dark {
  color: #faf5f1;
